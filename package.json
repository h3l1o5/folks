  "version": "1.0.0",
  "description": "a simple reactjs boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "client": "webpack-dev-server --open --devtool eval --progress --colors",
