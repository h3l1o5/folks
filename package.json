    "enzyme": "^2.9.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^20.0.4",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
